{
  "name": "eventracker",
  "version": "0.7.0",
  "type": "module",
  "scripts": {
    "dev": "tsc && vite build --watch"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1"
  },
  "description": "Extremely simple and useful event tracker library.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baspinarenes/eventracker.git"
  },
  "keywords": [
    "event"
  ],
  "author": "Enes Başpınar",
